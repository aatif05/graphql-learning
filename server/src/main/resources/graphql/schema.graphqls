type Query {
    hello: String
    getUser(name: String!): User
    getUsers: [User!]!
}

type User {
    id:ID!
    name: String!
    age: Int!
    address: Address
    posts: [Post!]!

}
type Address{
    street: String
    city: String
    zipCode: String
}
type Post {
    id: ID!
    title: String!
    content: String
}